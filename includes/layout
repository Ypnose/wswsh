# Page content

function page_header {
	print '<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />'
	if (( WSH_CSS == 1 )); then
		print "\t<link rel=\"stylesheet\" type=\"text/css\" href=\"/${WSH_CSSF}\" />"
	fi
	if [[ -n $WSH_TIT ]]; then
		print "\t<title>${WSH_TIT}${WSH_GENF_TITLE:+ | $WSH_GENF_TITLE}</title>"
	else
		print "\t<title>${WSH_GENF_TITLE}</title>"
	fi
	print '</head>
<body>'
}

function page_footer {
	print '</body>
</html>'
}
